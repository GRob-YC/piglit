[require]
GLSL >= 1.10

[vertex shader]
void main()
{
	gl_Position = gl_Vertex;
}

[fragment shader]
void main()
{
	for (int i = 0; i < 8; i++) {
		if (i > 4) {
			gl_FragColor = vec4(0.0, 1.0, 0.0, 0.0);
			continue;
		}
		gl_FragColor = vec4(1.0, 0.0, 0.0, 0.0);
	}
}

[test]
draw rect -1 -1 2 2
relative probe rgb (0.03, 0.25) (0.0, 1.0, 0.0)
